            //START UD-0689 04-21-2015 Marvin G
            //var isEligibilityChecked = $("input[id*='isPromoChecked']").val();
            var isEligibilityChecked = $("input[id*='isPromoChecked']").is(':checked');
            var subRecordId = $("input[id*='subscriptionRecId']").val();
