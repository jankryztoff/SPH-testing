    <validationRules>
        <fullName>No_COS_Request_First_of_Next_Month</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IsEOMCycle__c &amp;&amp;
CONTAINS(Order__r.Order_Type__c, &quot;Change of Subscription&quot;)&amp;&amp;
Start_Date__c  &lt;= 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() )+ 1, 1, 1 ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ))&amp;&amp; 
Start_Date__c &gt; 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() ), 12, (31 - $Setup.Days_Before_EOM__c.Number_Of_Days__c) ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ) - ($Setup.Days_Before_EOM__c.Number_Of_Days__c +1) ),
$User.Validation_Exempted__c &lt;&gt; TRUE)</errorConditionFormula>
        <errorMessage>Start Date date is within Bill Run period. Please select a date on or after the 2nd of next month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Pstop_Request_First_of_Next_Month</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IsEOMCycle__c &amp;&amp;
 Stop_Request_Date__c  &lt;= 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() )+ 1, 1, 1 ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ))&amp;&amp;
Stop_Request_Date__c   &gt; 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() ), 12, (31 - $Setup.Days_Before_EOM__c.Number_Of_Days__c) ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ) - ($Setup.Days_Before_EOM__c.Number_Of_Days__c +1) ),
$User.Validation_Exempted__c &lt;&gt; TRUE)</errorConditionFormula>
        <errorMessage>Stop date is within Bill Run period. Please select a date on or after the 2nd of next month</errorMessage>
    </validationRules>
