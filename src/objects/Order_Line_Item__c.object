    <fields>
        <fullName>IsEOMCycle__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Today() &lt;= 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() ), 12, 31 ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ) - 1 ) 
&amp;&amp; 
Today() &gt; 
IF( 
MONTH( TODAY()) = 12, 
DATE( YEAR( TODAY() ), 12, (31 - $Setup.Days_Before_EOM__c.Number_Of_Days__c) ), 
DATE( YEAR( TODAY() ), MONTH ( TODAY() ) + 1, 1 ) - ($Setup.Days_Before_EOM__c.Number_Of_Days__c +1) ),True, False)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IsEOMCycle?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
