    <validationRules>
        <fullName>All_Required_fields_are_populated</fullName>
        <active>true</active>
        <description>Required fields:
- Eligibility Rule Name
- Customer Status
These fields must be populated before the Eligibility Rule can be saved</description>
        <errorConditionFormula>ISPICKVAL(Customer_Status__c , &quot;&quot;) || ISBLANK(Name)</errorConditionFormula>
        <errorDisplayField>Customer_Status__c</errorDisplayField>
        <errorMessage>Required Fields are not populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Subscription_Rule_on_Contract</fullName>
        <active>true</active>
        <description>Contract / Subscription Rule must not be Contract End Date if Action = &quot;Can Add&quot;</description>
        <errorConditionFormula>AND( ISPICKVAL( Contract_Subscription_Rule__c, 
                 &quot;Contract End Date&quot;),
      ISPICKVAL( Action__c , &quot;Add&quot;) )</errorConditionFormula>
        <errorMessage>Contract / Subscription Rule must be Subscription End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Subscription_Rule_on_Contract_2</fullName>
        <active>true</active>
        <description>Contract / Subscription Rule must be N/A if Action= &quot;Add&quot;</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Contract_Status__c, 
                 &quot;With Contract&quot;),
       ISPICKVAL(Contract_Status__c, 
                 &quot;Without contract&quot;)),
       ISPICKVAL( Action__c , &quot;Add&quot;))</errorConditionFormula>
        <errorMessage>&quot;Contract&quot; field must be NA if Action = &quot;Can Add&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dates_Validation</fullName>
        <active>true</active>
        <description>Check the Start Date must not be later than the End Date</description>
        <errorConditionFormula>Effective_Start_Date__c &gt;  Effective_End_Date__c</errorConditionFormula>
        <errorDisplayField>Effective_Start_Date__c</errorDisplayField>
        <errorMessage>Check the Effective Start Date must not be later than the Effective End Date</errorMessage>
    </validationRules>
