    <validationRules>
        <fullName>One_Time_Charge_for_Free_Gifts</fullName>
        <active>true</active>
        <description>If gift type = free, One Time Charge field must be blank</description>
        <errorConditionFormula>ISPICKVAL(Gift_Type__c ,&apos;Free&apos;)&amp;&amp;One_Time_Charge__c&lt;&gt;0</errorConditionFormula>
        <errorMessage>There should be no One Time Charge for Free type Gifts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Time_Charge_for_Premium_Gifts</fullName>
        <active>true</active>
        <description>If gift type = premium, One Time Charge must be populated and value must be greater than $0</description>
        <errorConditionFormula>ISPICKVAL( Gift_Type__c , &quot;Premium&quot;)&amp;&amp;(ISNULL(One_Time_Charge__c)||ISBLANK(One_Time_Charge__c)||One_Time_Charge__c&lt;=0)</errorConditionFormula>
        <errorMessage>One Time Charge should be greater than $0 for Premium Gifts.</errorMessage>
    </validationRules>
