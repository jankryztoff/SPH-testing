    <validationRules>
        <fullName>Billing_Type_For_Grace_Period</fullName>
        <active>false</active>
        <description>Billing Type must be Recurring Postpaid if Package Type is Grace Period.</description>
        <errorConditionFormula>AND(TEXT(Package_Type__c)= &apos;Grace Period&apos;, ISBLANK(TEXT  (Billing_Type__c)))</errorConditionFormula>
        <errorDisplayField>Billing_Type__c</errorDisplayField>
        <errorMessage>Field must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Calc_Total_Subscription_Period</fullName>
        <active>false</active>
        <description>Calculation of total subscription period for extension discount package</description>
        <errorConditionFormula>ISPICKVAL(Package_Type__c, &apos;Discount&apos;) &amp;&amp; 
ISPICKVAL(Package_Sub_Type__c, &apos;Extension&apos;) &amp;&amp; 
ISPICKVAL(Subscription_Type__c, &apos;Termed&apos;) &amp;&amp; Subs_Period_Hidden__c &gt; Duration_Hidden__c</errorConditionFormula>
        <errorDisplayField>Subscription_Period__c</errorDisplayField>
        <errorMessage>Subscription Period exceeds the Effectivity Duration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EffectiveEndisBeforeEffectiveStart</fullName>
        <active>true</active>
        <description>Effective End Date cannot be before the effective start date</description>
        <errorConditionFormula>Effective_End_Date__c &lt; Effective_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Effective_End_Date__c</errorDisplayField>
        <errorMessage>Effective End Date is before the effective start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grace_Period_Evergreen</fullName>
        <active>true</active>
        <description>D-1537</description>
        <errorConditionFormula>AND(ISPICKVAL( Package_Type__c ,&apos;Grace Period&apos;),ISPICKVAL( Subscription_Type__c ,&apos;Evergreen&apos;))</errorConditionFormula>
        <errorDisplayField>Subscription_Type__c</errorDisplayField>
        <errorMessage>Grace Period Package should not have an Evergreen Subscription Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Month_Subscription_Period</fullName>
        <active>true</active>
        <description>Subscription Period must be one month if Package Type is Grace Period.</description>
        <errorConditionFormula>AND(ISPICKVAL(Package_Type__c,&apos;Grace Period&apos;),NOT(ISPICKVAL(Subscription_Period__c,&apos;1&apos;)),ISPICKVAL(Subscription_Type__c, &apos;Termed&apos;))</errorConditionFormula>
        <errorDisplayField>Subscription_Period__c</errorDisplayField>
        <errorMessage>Subscription Period must be one month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Code_Validation</fullName>
        <active>true</active>
        <description>Do not allow the user to change the Package Code.</description>
        <errorConditionFormula>IF(ISNULL( PRIORVALUE( Package_Code__c )),FALSE,ISCHANGED( Package_Code__c ) )</errorConditionFormula>
        <errorMessage>Package Code not allowed to be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Add_On</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    NOT(ISPICKVAL(Package_Type__c, &apos;Add On&apos;)),
    NOT(ISBLANK(Product_Family_for_Add_On_Packages__c))
)</errorConditionFormula>
        <errorDisplayField>Product_Family_for_Add_On_Packages__c</errorDisplayField>
        <errorMessage>Product Family should be NULL if Package type is not “Add on”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Bundle</fullName>
        <active>false</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Bundle&apos; &amp;&amp; Bundle_Discount_Count__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Only 1 Bundle Discount per Bundle Product is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Add_On</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Add On&apos; &amp;&amp; 
(Bundle_Discount_Count__c &gt; 0 || 
Bundle_Product_Count__c &gt; 0 || 
Extension_Discount_Count__c &gt; 0 || 
Extension_Product_Count__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only Base Product and Discount package items is allowed for Add On package type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Base</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Base&apos; &amp;&amp; (Bundle_Discount_Count__c &gt; 0 || Bundle_Product_Count__c &gt; 0 || Extension_Discount_Count__c &gt; 0 || Extension_Product_Count__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only Base Product package items is allowed for Base package type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Bundle</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Bundle&apos; &amp;&amp;  (Extension_Discount_Count__c &gt; 0 || 
Extension_Product_Count__c &gt; 0 || 
Discount_Count__c &gt; 0 ||
Base_Product_Count__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only Bundle Product and Bundle Discount package items are allowed for Bundle package type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Discount</fullName>
        <active>false</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Discount&apos; &amp;&amp; (Bundle_Discount_Count__c &gt; 0 || 
Bundle_Product_Count__c &gt; 0 || 
Discount_Count__c &gt; 0 || 
Base_Product_Count__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only Extension Product and Extension Discount package items are allowed for Discount package type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Discount_Extension</fullName>
        <active>true</active>
        <description>Restricts a Package to only 1 &quot;Base Product&quot; , 1 &quot;Discount&quot; , 1&quot;Extension Product&quot;, and 1&quot;Extension Discount&quot;  Package Item.</description>
        <errorConditionFormula>ISPICKVAL(Package_Sub_Type__c, &apos;Extension&apos;) &amp;&amp; TEXT(Package_Type__c) = &apos;Discount&apos; &amp;&amp; (Base_Product_Count__c &gt; 1 || Discount_Count__c &gt; 1 || Extension_Product_Count__c &gt; 1 || Extension_Discount_Count__c &gt; 1)</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only 1 &quot;Base Product&quot;,  1 &quot;Discount&quot;, 1 &quot;Extension Product&quot;, and 1 &quot;Extension Discount&quot; Package Item is allowed for a Package of type &quot;Discount&quot; with Sub Type &quot;Extension&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Discount_Test</fullName>
        <active>true</active>
        <description>Restricts a Package to only 1 &quot;Base Product&quot;  and 1 &quot;Discount&quot; Package Item.</description>
        <errorConditionFormula>(ISPICKVAL(Package_Sub_Type__c, &apos;Single&apos;) || ISPICKVAL(Package_Sub_Type__c, &apos;&apos;)) &amp;&amp; ((TEXT(Package_Type__c) = &apos;Discount&apos; || ISPICKVAL(Package_Type__c, &apos;Add On&apos;)) &amp;&amp; (Base_Product_Count__c &gt; 1 || Discount_Count__c &gt; 1))</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only 1 &quot;Base Product&quot; and 1 &quot;Discount&quot; Package Item are allowed for a Package of type &quot;Discount&quot; or &quot;Add On&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Change_Discount_Tiered</fullName>
        <active>true</active>
        <description>Restricts a Discount Package of &quot;Tiered&quot; subtype to only 1 &quot;Base Product&quot;</description>
        <errorConditionFormula>ISPICKVAL(Package_Sub_Type__c, &apos;Tiered&apos;) &amp;&amp; TEXT(Package_Type__c) = &apos;Discount&apos; &amp;&amp; Base_Product_Count__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Conflict with Package Items that already exist. Only 1 &quot;Base Product&quot;  is allowed for a Package of type &quot;Discount&quot; with &quot;Tiered&quot; Sub Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Discount</fullName>
        <active>true</active>
        <description>A Package Sub Type must be chosen if the Package Type specified is &quot;Discount&quot;</description>
        <errorConditionFormula>ISPICKVAL(Package_Type__c, &apos;Discount&apos;) &amp;&amp;  ISPICKVAL(Package_Sub_Type__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Package_Sub_Type__c</errorDisplayField>
        <errorMessage>A Package Sub Type must be chosen for Package Type: Discount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Discount_Package_Sub_Ext</fullName>
        <active>true</active>
        <description>Displays an error message if Package Type is &quot;Discount&quot;, Package Sub Type is &quot;Extension&quot;, and Subscription Type is &quot;Evergreen&quot;</description>
        <errorConditionFormula>ISPICKVAL(Package_Type__c, &apos;Discount&apos;) &amp;&amp; ISPICKVAL(Package_Sub_Type__c, &apos;Extension&apos;) &amp;&amp;  ISPICKVAL(Subscription_Type__c, &apos;Evergreen&apos;)</errorConditionFormula>
        <errorDisplayField>Subscription_Type__c</errorDisplayField>
        <errorMessage>Subscription Type should not be &quot;Evergreen&quot; if Package Type is &quot;Discount&quot; and Package Sub Type is &quot;Extension&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Extension_Discount</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Discount&apos; &amp;&amp; Extension_Discount_Count__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Only 1 Extension Discount must be allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Type_Extension_Product</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Package_Type__c) = &apos;Discount&apos; &amp;&amp; Extension_Product_Count__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Package_Type__c</errorDisplayField>
        <errorMessage>Only 1 Extension Product must be allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductFamilyforAddOnPackages</fullName>
        <active>false</active>
        <description>Product Family field must be populated for Add-On Packages</description>
        <errorConditionFormula>ISBLANK(Product_Family_for_Add_On_Packages__c)  &amp;&amp;   ISPICKVAL( Package_Type__c , &quot;Add On&quot;)</errorConditionFormula>
        <errorDisplayField>Product_Family_for_Add_On_Packages__c</errorDisplayField>
        <errorMessage>Please enter the Product family for which this package is eligible.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Family_is_Required</fullName>
        <active>false</active>
        <description>Product Family is required if Package Type is Grace Period.</description>
        <errorConditionFormula>AND(ISPICKVAL(Package_Type__c, &apos;Grace Period&apos;),Product_Family_Current_Package__c = null)</errorConditionFormula>
        <errorDisplayField>Product_Family_Current_Package__c</errorDisplayField>
        <errorMessage>Field must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubscriptionPeriodforSubTypeEvergreen</fullName>
        <active>true</active>
        <description>Subscription Period must be filled for Subscription Type: Termed.</description>
        <errorConditionFormula>ISPICKVAL(Subscription_Type__c, &quot;Evergreen&quot;)  &amp;&amp; ISPICKVAL(Subscription_Period__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Subscription_Period__c</errorDisplayField>
        <errorMessage>If subscription type is evergreen, the subscription period must be N/A</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubscriptionPeriodforSubTypeTerm</fullName>
        <active>true</active>
        <description>Subscription Period must be filled for Subscription Type: Termed.</description>
        <errorConditionFormula>ISPICKVAL(Subscription_Type__c, &quot;Termed&quot;)  &amp;&amp; ISPICKVAL(Subscription_Period__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Subscription_Period__c</errorDisplayField>
        <errorMessage>Subscription period is not filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TaggedtoPromoforDiscountAddOn</fullName>
        <active>true</active>
        <description>Tagged to Promo should be filled only for Discount and Add-On packages. Base and Bundle packages must not be ticked for this field</description>
        <errorConditionFormula>Tagged_to_Promo_for_Add_On_Discount__c &amp;&amp; 

(ISPICKVAL( Package_Type__c , &quot;Base&quot;) ||
ISPICKVAL( Package_Type__c, &quot;Bundle&quot;))</errorConditionFormula>
        <errorDisplayField>Tagged_to_Promo_for_Add_On_Discount__c</errorDisplayField>
        <errorMessage>Tagged to Promo must not be ticked for Base or Bundle packages</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Termed_Subscription_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Package_Type__c, &apos;Grace Period&apos;),ISPICKVAL(Subscription_Type__c, &apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Subscription_Type__c</errorDisplayField>
        <errorMessage>Field must be populated.</errorMessage>
    </validationRules>
</CustomObject>
