    <validationRules>
        <fullName>Amount_Paid</fullName>
        <active>true</active>
        <description>Only accepts positive value</description>
        <errorConditionFormula>AND(
TEXT(Payment_Mode__c) = &apos;Cash&apos;, 
OR(
TEXT(Status__c) = &apos;Draft&apos;, 
TEXT(Status__c) = &apos;Pending&apos;,
 TEXT(Status__c) = &apos;Provisioning&apos;, 
TEXT(Status__c) = &apos;Pending Vendor Assignment&apos;, 
TEXT(Status__c) = &apos;Close&apos;, 
TEXT(Status__c) = &apos;Closed&apos;
), 
IF(
(Total_Due_to_SPH__c - Recurring_Inc_Del_Charge__c) != 0,
OR(
ISBLANK(Amount_Paid__c), 
Amount_Paid__c &lt; (Total_Due_to_SPH__c - Recurring_Inc_Del_Charge__c)
),
false
)
)</errorConditionFormula>
        <errorDisplayField>Amount_Paid__c</errorDisplayField>
        <errorMessage>Amount should be greater than total due amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Account_Required</fullName>
        <active>true</active>
        <description>[KCampang] 08/13/2014. SCR_SubscriptionDetail_VR_7. Required Billing Account.</description>
        <errorConditionFormula>ISNULL( Billing_Account__c) &amp;&amp;   CONTAINS(Account__r.RecordType.Name,&apos;Direct&apos;) &amp;&amp;   ISPICKVAL(Billing_Account_Creation_Mode__c, &apos;Use existing billing account&apos;)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Billing Account is required when Account Record is of type Direct and use existing billing account mode was selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Contact_is_required_for_Invoice</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(Billing_Contact__c)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Billing Contact is required when creating Invoice Billing Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CS_Case_Sub_Status</fullName>
        <active>true</active>
        <description>Case Sub status for this subscription must be ready for submission or Urgent Start Approved before the Order can be send to Zuora.</description>
        <errorConditionFormula>AND ( 
	OR( ISPICKVAL(Case_Owner__r.Sub_Status__c, &apos;Ready for Submission&apos;), ISPICKVAL(Case_Owner__r.Sub_Status__c, &apos;Urgent Start Approved&apos;)),  
	NOT( OR( ISPICKVAL(Status__c, &apos;New&apos;), ISPICKVAL(Status__c, &apos;Draft&apos;), ISPICKVAL(Status__c, &apos;Void&apos;) ) ) 
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Unable to submit to Zuora because case tied is not ready for submission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount</fullName>
        <active>true</active>
        <description>Amount should be greater than zero</description>
        <errorConditionFormula>AND(
TEXT(Payment_Mode__c) = &apos;Check&apos;, 
OR(
TEXT(Status__c) = &apos;Draft&apos;,
TEXT(Status__c) = &apos;Pending&apos;, 
TEXT(Status__c) = &apos;Provisioning&apos;, 
TEXT(Status__c) = &apos;Pending Vendor Assignment&apos;, 
TEXT(Status__c) = &apos;Close&apos;, 
TEXT(Status__c) = &apos;Closed&apos;
), 
IF(
Total_Due_to_SPH__c != 0,
OR(
ISBLANK(Check_Amount__c),
Check_Amount__c &lt; Total_Due_to_SPH__c
),
false
),
ISPICKVAL(Billing_Account_Creation_Mode__c, &apos;Create New Billing Account&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Check_Amount__c</errorDisplayField>
        <errorMessage>Amount must be greater than total amount due.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CostCentreRequired</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Payment_Mode__c, &quot;Other&quot;) &amp;&amp;  ISPICKVAL(Other_Payment_Mode__c, &quot;Internal Charge&quot;) &amp;&amp;  ISBLANK(Cost_Centre__c)</errorConditionFormula>
        <errorMessage>Cost Centre is required when Other Payment Mode is Internal Charge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Debt_Management_Treatment_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Create_ARMS_Account_Only__c = TRUE &amp;&amp;  ISPICKVAL(Debt_Management_Treatment__c, &apos;NONE&apos;), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Debt_Management_Treatment__c</errorDisplayField>
        <errorMessage>&apos;Debt Mgmt Treatment&apos; field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Debt_Mgmt_Treatment_Bus_Profile_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Create_ARMS_Customer_Account__c = TRUE, ISBLANK(TEXT(Debt_Management_Treatment__c)), ISBLANK(TEXT(Business_Profile__c)))</errorConditionFormula>
        <errorDisplayField>Create_ARMS_Customer_Account__c</errorDisplayField>
        <errorMessage>&apos;Debt Management Treatment&apos; and &apos;Business Profile&apos; fields are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GLCodeRequiredForInternalCharges</fullName>
        <active>true</active>
        <description>If other payment mode is internal charge, GL Code is required.</description>
        <errorConditionFormula>ISPICKVAL(Payment_Mode__c, &quot;Other&quot;) &amp;&amp; ISPICKVAL(Other_Payment_Mode__c, &quot;Internal Charge&quot;) &amp;&amp;
ISPICKVAL(Billing_Account_Creation_Mode__c , &quot;Create New Billing Account&quot;) &amp;&amp;
ISBLANK(GL_Code__c)</errorConditionFormula>
        <errorDisplayField>GL_Code__c</errorDisplayField>
        <errorMessage>Please input GL Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OrderSubEndDate</fullName>
        <active>true</active>
        <description>Order Detail Sub End Date cannot be less than Start Date
2509</description>
        <errorConditionFormula>Subscription_End_Date__c &lt;  Subscription_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Subscription_End_Date__c</errorDisplayField>
        <errorMessage>Subscription End Date must not less than Subscription Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Mode_Validation</fullName>
        <active>false</active>
        <description>Validate user input for the payment information inputted. For Example, For Recurring Postpaid, User can only select Credit Card or Invoice as Payment Mode.</description>
        <errorConditionFormula>AND( ISPICKVAL(Billing_Type__c , &apos;Recurring Postpaid&apos;), NOT(AND( ISPICKVAL(Payment_Mode__c, &apos;Credit Card&apos;), ISPICKVAL(Payment_Mode__c, &apos;Invoice&apos;) ) ) )</errorConditionFormula>
        <errorMessage>Payment Mode is not allowed for selected Payment Method and Payment Mode.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PaypalPaymentModeValidation</fullName>
        <active>true</active>
        <description>New customers/subscriptions cannot avail of PayPal as a Payment Type.</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c , &apos;New&apos;) , ISPICKVAL(Payment_Mode__c , &apos;Paypal&apos;) , ISPICKVAL( Payment_Type__c , &apos;Prepaid&apos;) )</errorConditionFormula>
        <errorDisplayField>Payment_Mode__c</errorDisplayField>
        <errorMessage>New customers/subscriptions cannot avail of PayPal as a Payment Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PostpaidBillingPeriodValidation</fullName>
        <active>true</active>
        <description>New customers/subscriptions will not be able to select this billing period.</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c , &apos;New&apos;) ,  OR(ISPICKVAL(Payment_Mode__c,&apos;Credit Card&apos;),ISPICKVAL(Payment_Mode__c , &apos;Invoice&apos;)),
ISPICKVAL( Payment_Type__c , &apos;Postpaid&apos;) )</errorConditionFormula>
        <errorDisplayField>Payment_Type__c</errorDisplayField>
        <errorMessage>New customers/subscriptions will not be able to select this billing period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_cannot_be_zero</fullName>
        <active>true</active>
        <description>Quantity cannot be reduce to zero during change of susbscription.</description>
        <errorConditionFormula>AND( New_Quantity__c = 0,  
ISPICKVAL(Category__c, &apos;Subscription Request&apos;))</errorConditionFormula>
        <errorDisplayField>New_Quantity__c</errorDisplayField>
        <errorMessage>Quantity cannot be reduce to zero during change of susbscription.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Billing_Account</fullName>
        <active>false</active>
        <description>Billing Account must be provided if the Account is Direct.</description>
        <errorConditionFormula>(NOT(Account__r.RecordType.DeveloperName = &apos;Direct_Corporate&apos;) || NOT(Account__r.RecordType.DeveloperName = &apos;Direct_Individual&apos;) || NOT(Account__r.RecordType.DeveloperName = &apos;Direct_SPH_Inter_Division&apos;))  &amp;&amp;
ISBLANK(Billing_Account__c)</errorConditionFormula>
        <errorDisplayField>Billing_Account__c</errorDisplayField>
        <errorMessage>Billing Account must be provided the Account is Direct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Billing_Contact</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Billing_Contact__c),  TEXT(Billing_Account_Creation_Mode__c) = &apos;Create New Billing Account&apos;)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Billing Contact is required when creating New Billing Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Delivery_Charge</fullName>
        <active>false</active>
        <description>[KCampang]. 8/13/2014.SCR_SubscriptionDetail_VR_9. Required Subscriber Charge.</description>
        <errorConditionFormula>ISNULL( Delivery_Charge__c) &amp;&amp;
( ISPICKVAL( Delivery_Charge_Type__c , &apos;H&apos;) ||
 ISPICKVAL( Delivery_Charge_Type__c , &apos;C&apos;) ||
 ISPICKVAL( Delivery_Charge_Type__c , &apos;L&apos;) ||
 ISPICKVAL( Delivery_Charge_Type__c , &apos;S&apos;) ||
 ISPICKVAL( Delivery_Charge_Type__c , &apos;9&apos;))</errorConditionFormula>
        <errorDisplayField>Delivery_Charge__c</errorDisplayField>
        <errorMessage>Delivery Charge is required when the selected delivery charge type is among the following: H, C, L, S, or 9.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Informed</fullName>
        <active>false</active>
        <description>Requires user to check Informed checkbox
[KCampang] 08/11/2014. Updated error condition with record type considerations.

[JohnD 10/29] Copied updated condition from DV1.</description>
        <errorConditionFormula>Account__r.RecordType.Name = &apos;Vendor Subscriber&apos;
&amp;&amp;
Informed_Customer__c = FALSE &amp;&amp; NOT(ISPICKVAL(Sub_Category__c,&apos;Stop Subscription&apos;))</errorConditionFormula>
        <errorDisplayField>Informed_Customer__c</errorDisplayField>
        <errorMessage>Have you informed vendor?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Reason</fullName>
        <active>true</active>
        <description>Requires user to select a reason
[KCampang] 8/11/2014. Added a record type condition.</description>
        <errorConditionFormula>Account__r.RecordType.Name = &apos;Vendor Subscriber&apos;
&amp;&amp;(ISNULL(TEXT( Reason__c )))</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Reason must be provided when the Account is a Vendor Subscriber.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Subscriber_Contact</fullName>
        <active>true</active>
        <description>[KCampang] 08/15/2014.  Subscriber Contact required for direct if option selected is ‘Create new billing Account’</description>
        <errorConditionFormula>CONTAINS(Account__r.RecordType.Name, &apos;Direct&apos;) &amp;&amp;  ISPICKVAL( Billing_Account_Creation_Mode__c , &apos;Create New Billing Account&apos;) &amp;&amp;   ISNULL(Billing_Contact__c)</errorConditionFormula>
        <errorDisplayField>Billing_Contact__c</errorDisplayField>
        <errorMessage>Subscriber/Billing Contact is required if account is Direct and selected Creation Mode was &quot;Create New Billing Account&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Subscriber_Recipient</fullName>
        <active>true</active>
        <description>Requires user to enter a recipient
[KCampang] 8/11/2014. SCR_ OrderEntry_Vendor_VR_1. Requires user to enter a recipient</description>
        <errorConditionFormula>(Recipient_Contact__c = NULL)
&amp;&amp;
Account__r.RecordType.Name = &apos;Vendor Subscriber&apos;&amp;&amp;  NOT(Order_Type__c = &apos;Temp Stop&apos;)</errorConditionFormula>
        <errorDisplayField>Recipient_Contact__c</errorDisplayField>
        <errorMessage>Please enter recipient.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Vendor_Credit</fullName>
        <active>false</active>
        <description>Requires user to input vendor credit.
[KCampang] 08/13/2014. Updated error condition.</description>
        <errorConditionFormula>(ISNULL(Vendor_Credit__c )) &amp;&amp; (ISPICKVAL( Delivery_Charge_Type__c, &apos;H&apos;) ||
ISPICKVAL( Delivery_Charge_Type__c, &apos;C&apos;) ||
ISPICKVAL( Delivery_Charge_Type__c, &apos;L&apos;) || 
ISPICKVAL( Delivery_Charge_Type__c, &apos;S&apos;) || 
ISPICKVAL( Delivery_Charge_Type__c, &apos;N&apos;) || 
ISPICKVAL( Delivery_Charge_Type__c, &apos;9&apos;))</errorConditionFormula>
        <errorDisplayField>Vendor_Credit__c</errorDisplayField>
        <errorMessage>Vendor Credit is required when Delivery Charge is among the following types: H, C, L, S, N, or 9.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Effective_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Effective_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Start Date should be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>New_Quantity__c &lt; 0</errorConditionFormula>
        <errorDisplayField>New_Quantity__c</errorDisplayField>
        <errorMessage>Quantity invalid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Subscription_End_Date</fullName>
        <active>true</active>
        <description>If package is under promotion, default end date is automatically defined by the system. User cannot edit this field.</description>
        <errorConditionFormula>AND(NOT( ISNULL( Subscription__r.Main_Package_1__r.Number_Of_Promotions__c ) 
), ISCHANGED( Subscription_End_Date__c ) )</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Subscription End Date cannot be changed based from the selected promotion.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Subscription_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Subscription_Start_Date__c &lt;&gt; null , Subscription_Start_Date__c &lt;= today())</errorConditionFormula>
        <errorDisplayField>Subscription_Start_Date__c</errorDisplayField>
        <errorMessage>Subscription Start Date should be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Other_payment_Mode</fullName>
        <active>true</active>
        <description>Validation if payment mode is Other; therefore it requires Other payment mode</description>
        <errorConditionFormula>AND(
ISPICKVAL(Other_Payment_Mode__c, &apos;&apos;),
ISPICKVAL(Payment_Mode__c, &apos;Other&apos;),  ISPICKVAL(Billing_Account_Creation_Mode__c, &apos;Create New Billing Account&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Other Payment Mode is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Voiding_Order</fullName>
        <active>true</active>
        <description>Order can only be voided if the status is Draft.</description>
        <errorConditionFormula>AND(
  ISCHANGED(Status__c),
    AND(NOT(ISPICKVAL(PRIORVALUE(Status__c), &quot;Draft&quot;)), NOT(ISPICKVAL(PRIORVALUE(Order_Type1__c), &quot;Change of Subscription&quot;))) ,    
    (ISPICKVAL(Status__c, &quot;Void&quot;))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Voiding Order is not permitted.</errorMessage>
    </validationRules>
