    <fields>
        <fullName>Total_Due_to_Customer__c</fullName>
        <externalId>false</externalId>
        <formula>IF( CONTAINS(Order_Type__c, &apos;Start&apos;) , 
IF((Admin_Charge__c + Subscription_Rate__c) &lt; Credit_Balance__c ,
Credit_Balance__c - (Admin_Charge__c + Subscription_Rate__c),
0), 
IF(( Admin_Charge__c + Subscription_Rate__c + Total_Full_Prepaid__c) &lt; Credit_Amount__c ,
Credit_Amount__c - (Admin_Charge__c + Subscription_Rate__c + Total_Full_Prepaid__c), 
0)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>For recurring subscriptions, the &quot;total due to customer&quot; amount is derived by credit balance less monthly delivery fee. Do ignore the amount shown in this page.</inlineHelpText>
        <label>Total Due to Customer</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Due_to_SPH__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS(Order_Type__c, &apos;Start&apos;) ,
IF (((Full_Prepaid_Inc_Del_Charge__c + Recurring_Inc_Del_Charge__c + Total_Gift_Charge__c + Admin_Charge__c) - Credit_Balance__c) &lt;= 0,
0,
(Full_Prepaid_Inc_Del_Charge__c + Recurring_Inc_Del_Charge__c + Total_Gift_Charge__c + Admin_Charge__c) - Credit_Balance__c) ,
IF(((Full_Prepaid_Inc_Del_Charge__c + Recurring_Inc_Del_Charge__c + Total_Gift_Charge__c + Admin_Charge__c) - Credit_Amount__c) &lt;= 0,
0,
(Full_Prepaid_Inc_Del_Charge__c + Recurring_Inc_Del_Charge__c + Total_Gift_Charge__c + Admin_Charge__c) - Credit_Amount__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Due to SPH</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
