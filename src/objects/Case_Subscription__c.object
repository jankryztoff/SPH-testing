        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>isComplaint__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Used in filtering Subscription field. If true, does not show &apos;Delivery&apos; subscriptions.</description>
        <externalId>false</externalId>
        <label>isComplaint</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Case Subscription</label>
    <listViews>
        <fullName>All</fullName>
        <columns>Subscription__c</columns>
        <columns>Subscription_Name_Mapping__c</columns>
        <columns>Publication_Code__c</columns>
        <columns>Case__c</columns>
        <columns>NAME</columns>
        <columns>Account__c</columns>
        <columns>Case_Record_Type_ID__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>CS{0000000000}</displayFormat>
        <label>Case Subscription Name</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Case Subscriptions</pluralLabel>
    <searchLayouts>
        <lookupDialogsAdditionalFields>Vendor_Debit__c</lookupDialogsAdditionalFields>
        <searchFilterFields>Vendor_Debit__c</searchFilterFields>
        <searchResultsAdditionalFields>Vendor_Debit__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>ApprovedRejectedGoodwillNewspaper</fullName>
        <active>true</active>
        <description>D-1951</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Case__r.Goodwill_Approval_Status__c,&apos;Approved&apos;),ISPICKVAL(Case__r.Goodwill_Approval_Status__c,&apos;Rejected&apos;)), ISCHANGED( Goodwill_Amount__c))</errorConditionFormula>
        <errorMessage>Approved/Rejected Goodwill Amount cannot be updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloseCaseValidation</fullName>
        <active>false</active>
        <description>Case cannot be close if there is a goodwill amount and goodwill approval status is not approved.</description>
        <errorConditionFormula>NOT(ISNULL(Goodwill_Amount__c )) &amp;&amp; NOT( ISPICKVAL( Case__r.Goodwill_Approval_Status__c , &quot;Approved&quot;)) &amp;&amp; ISPICKVAL(  Case__r.Status , &quot;Closed&quot;)</errorConditionFormula>
        <errorMessage>Case cannot be closed if goodwill amount is entered and not approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Copies_Subscription_Copies</fullName>
        <active>true</active>
        <description>(No. of Copies * No. of Days in Complaint Period)  should be greater or equal to Complaint Copies</description>
        <errorConditionFormula>IF(NOT(ISNULL(Complaint_Copies__c)), Subscription__r.Number_of_Copies__c &lt; Complaint_Copies__c,FALSE)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Complaint copies cannot be greater than the Subscription Copies</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Dates_cannot_be_future_dates</fullName>
        <active>true</active>
        <errorConditionFormula>Complaint_From__c &gt; today()  ||  Complaint_To__c &gt; today()</errorConditionFormula>
        <errorMessage>Complaint Dates cannot be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_SubCantBeEditedByCSOAdmin</fullName>
        <active>true</active>
        <description>Subscription field cannot be edited by CSO admin when editing Complaint Cases.</description>
        <errorConditionFormula>AND($Profile.Name = &apos;CSO Admin&apos;,  ISCHANGED( Subscription__c ),  Case__r.RecordType.DeveloperName = &apos;Complaint Magazine&apos; )</errorConditionFormula>
        <errorMessage>CSO Admin cant edit subscription. Please contact CSO.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_To_Date_Validation</fullName>
        <active>true</active>
        <description>Complaint to date must not be less than Complaint From date</description>
        <errorConditionFormula>Complaint_To__c &lt; Complaint_From__c</errorConditionFormula>
        <errorDisplayField>Complaint_To__c</errorDisplayField>
        <errorMessage>Complaint to date must not be less than Complaint From date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Copies_cannot_be_negative</fullName>
        <active>true</active>
        <description>Credit Copies = Complaint Copies - Replacement Copies</description>
        <errorConditionFormula>IF(NOT(ISNULL(Replacement_Copies__c)),
Complaint_Copies__c &lt; Replacement_Copies__c,
FALSE
)</errorConditionFormula>
        <errorMessage>Replacement Copies cannot be greater than Complaint Copies</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplaceFromDateMustBeFutureDate</fullName>
        <active>false</active>
        <description>Replace to date should not be a past date. Refer to UD-0616</description>
        <errorConditionFormula>Replace_From__c &lt; today()</errorConditionFormula>
        <errorDisplayField>Replace_From__c</errorDisplayField>
        <errorMessage>Replace From Date should not be in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Replace_From_Date_Required</fullName>
        <active>true</active>
        <description>Replace from date must be required if Replace to date has a value.</description>
        <errorConditionFormula>AND(ISBLANK(Replace_From__c), NOT(ISBLANK(Replace_To__c)))</errorConditionFormula>
        <errorDisplayField>Replace_From__c</errorDisplayField>
        <errorMessage>Replace from date must be required if Replace to date has a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Replace_To_Date_Required</fullName>
        <active>true</active>
        <description>Replace to date must be required if Replace from date has a value.</description>
        <errorConditionFormula>AND(ISBLANK(Replace_To__c), NOT(ISBLANK(Replace_From__c)))</errorConditionFormula>
        <errorDisplayField>Replace_To__c</errorDisplayField>
        <errorMessage>Replace to date must be required if Replace from date has a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Replace_To_Date_Validation</fullName>
        <active>true</active>
        <description>Replace to date must be &gt;= Replace from date</description>
        <errorConditionFormula>Replace_To__c  &lt; Replace_From__c</errorConditionFormula>
        <errorDisplayField>Replace_To__c</errorDisplayField>
        <errorMessage>Replace To Date must not be less than Replace From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementFromDateWithinComplaintPeriod</fullName>
        <active>true</active>
        <errorConditionFormula>((Replace_From__c &gt;  Complaint_To__c &amp;&amp; Replace_From__c &gt; Complaint_From__c) || (Replace_From__c &lt; Complaint_To__c &amp;&amp; Replace_From__c &lt; Complaint_From__c))</errorConditionFormula>
        <errorDisplayField>Replace_From__c</errorDisplayField>
        <errorMessage>Replacement From should be within Complaint Period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementFromNotGreaterThan14</fullName>
        <active>false</active>
        <errorConditionFormula>Case__r.Replacement_Required__c &amp;&amp; Replace_From__c &lt; (TODAY()- 14)</errorConditionFormula>
        <errorDisplayField>Replace_From__c</errorDisplayField>
        <errorMessage>Replacement Issue From Date should not be older than  14 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementFromfieldRequired</fullName>
        <active>false</active>
        <description>Check Replacement Issue Date if Replacement is required for Complaint case.</description>
        <errorConditionFormula>ISBLANK(Replace_From__c) &amp;&amp; Case__r.Replacement_Required__c &amp;&amp;  $Profile.Name != &apos;CSO Admin&apos;</errorConditionFormula>
        <errorDisplayField>Replace_From__c</errorDisplayField>
        <errorMessage>Replacement Issue Date is required if Replacement Required checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementIssueDateFieldsRequired</fullName>
        <active>true</active>
        <description>Check Replacement Issue Date if Replacement is required for Complaint case.</description>
        <errorConditionFormula>(ISBLANK(Replace_To__c) || ISBLANK(Replace_From__c)) &amp;&amp; Case__r.Replacement_Required__c</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Replacement Issue Dates are required if Replacement Required checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementReqReplacementCopies</fullName>
        <active>true</active>
        <errorConditionFormula>Case__r.RecordType.Name = &apos;Complaint - Newspaper&apos;
&amp;&amp; 
IF (Case__r.Replacement_Required__c = FALSE, Replacement_Copies__c &lt;&gt; null || Replacement_Copies__c &lt;&gt; 0, FALSE)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Replacement Required checkbox is unchecked, Replacement Copies should be blank or zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementToDateIsNotToday</fullName>
        <active>true</active>
        <errorConditionFormula>((Replace_To__c &gt;  Complaint_To__c &amp;&amp; Replace_To__c &gt; Complaint_From__c) || (Replace_To__c &lt; Complaint_To__c &amp;&amp; Replace_To__c &lt; Complaint_From__c))</errorConditionFormula>
        <errorDisplayField>Replace_To__c</errorDisplayField>
        <errorMessage>Replacement To should be within Complaint Period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReplacementTofieldRequired</fullName>
        <active>false</active>
        <description>Check Replacement Issue Date if Replacement is required for Complaint case.</description>
        <errorConditionFormula>ISBLANK(Replace_To__c) &amp;&amp; Case__r.Replacement_Required__c &amp;&amp; $Profile.Name != &apos;CSO Admin&apos;</errorConditionFormula>
        <errorDisplayField>Replace_To__c</errorDisplayField>
        <errorMessage>Replacement Issue Date is required if Replacement Required checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubscriptionEdit</fullName>
        <active>true</active>
        <errorConditionFormula>OR($Profile.Name  = &apos;CSO Admin&apos;, CONTAINS($Profile.Name , &apos;Vendor&apos;)) &amp;&amp; ISCHANGED(Subscription__c)</errorConditionFormula>
        <errorMessage>User cannot update subscription.</errorMessage>
    </validationRules>
</CustomObject>
