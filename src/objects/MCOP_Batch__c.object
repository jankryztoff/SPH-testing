    <validationRules>
        <fullName>ContEDateFrom_and_ConEDateTo_Mandatory</fullName>
        <active>true</active>
        <description>“Contract End Date From”  and “Contract End Date To” can only have value if “Is On Contract?” is checked</description>
        <errorConditionFormula>AND( On_Contract__c = FAlSE,  OR(NOT(ISNULL(Contract_End_Date_From__c)), NOT(ISNULL(Contract_End_Date_To__c))))
||
AND( On_Contract__c = TRUE,  NOT(ISNULL(Contract_End_Date_From__c)) != NOT(ISNULL(Contract_End_Date_To__c)))</errorConditionFormula>
        <errorDisplayField>On_Contract__c</errorDisplayField>
        <errorMessage>Invalid Contract End Date Range</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContEndDateTo_cnotbefore_ContEndDateFrom</fullName>
        <active>true</active>
        <description>“Contract End Date To” cannot before “Contract End Date From”</description>
        <errorConditionFormula>Contract_End_Date_From__c &gt; Contract_End_Date_To__c</errorConditionFormula>
        <errorDisplayField>Contract_End_Date_To__c</errorDisplayField>
        <errorMessage>Contract End Date To cannot before Contract End Date From</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ConvEffeDate_greater_PrefrJobStartTime1</fullName>
        <active>true</active>
        <description>“Conversion Effective Date” shall be greater than “Preferred Job Start Time” + 1</description>
        <errorConditionFormula>NOT(Effective_Date__c  &gt;  DATEVALUE( Start_Time__c +1))</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>Conversion Effective Date should be greater than Preferred Job Start Time + 1 Day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ConversinEffDate_after_ContractEndDateTo</fullName>
        <active>true</active>
        <description>“Conversion Effective Date” shall be after the “Contract End Date To”</description>
        <errorConditionFormula>NOT( Effective_Date__c &gt;  Contract_End_Date_To__c )</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>Conversion Effective Date should be after the Contract End Date To</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>From_and_To_Package_Same_BillingType</fullName>
        <active>true</active>
        <description>“From Package” and “To Package” should be Same BillingType</description>
        <errorConditionFormula>NOT( TEXT(From_Package__r.Billing_Type__c) = TEXT(To_Package__r.Billing_Type__c) )</errorConditionFormula>
        <errorDisplayField>From_Package__c</errorDisplayField>
        <errorMessage>From Package and To Package should be Same BillingType</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>From_and_To_Package_cannot_Bundle_AddOn</fullName>
        <active>true</active>
        <description>“From Package” and “To Package” cannot be a Bundle or Add-on Package</description>
        <errorConditionFormula>OR(  ISPICKVAL(From_Package__r.Package_Type__c, &apos;Bundle&apos;)  ,ISPICKVAL(From_Package__r.Package_Type__c, &apos;Add On&apos;) ,ISPICKVAL(To_Package__r.Package_Type__c, &apos;Bundle&apos;)  ,ISPICKVAL(To_Package__r.Package_Type__c, &apos;Add On&apos;))</errorConditionFormula>
        <errorDisplayField>From_Package__c</errorDisplayField>
        <errorMessage>From Package and To Package cannot be a Bundle or Add-on Package</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>From_and_To_Package_cannot_Full_Prepaid</fullName>
        <active>true</active>
        <description>“From Package” and “To Package” cannot be a Full Prepaid Package</description>
        <errorConditionFormula>AND(  ISPICKVAL(From_Package__r.Billing_Type__c, &apos;Full Prepaid&apos;) , ISPICKVAL(To_Package__r.Billing_Type__c , &apos;Full Prepaid&apos;) )</errorConditionFormula>
        <errorDisplayField>From_Package__c</errorDisplayField>
        <errorMessage>From Package and To Package cannot be a Full Prepaid Package</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreferredJobStartTime_greaterthan_NOW</fullName>
        <active>true</active>
        <description>“Preferred Job Start Time” shall be greater than NOW</description>
        <errorConditionFormula>Start_Time__c  &lt;  NOW()</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Preferred Job Start Time should be greater than NOW</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubQtyTo_value_cannot_lesthan_SubQtyFrom</fullName>
        <active>true</active>
        <description>“Subscription Quantity To” value cannot be less than “Subscription Quantity From”</description>
        <errorConditionFormula>Sub_Qty_From__c &gt; Sub_Qty_To__c</errorConditionFormula>
        <errorDisplayField>Sub_Qty_To__c</errorDisplayField>
        <errorMessage>Subscription Quantity To value cannot be less than Subscription Quantity From</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_Qty_From_value_greatertha_0</fullName>
        <active>true</active>
        <description>“Subscription Quantity From” value shall be greater than 0</description>
        <errorConditionFormula>Sub_Qty_From__c  &lt;   1</errorConditionFormula>
        <errorDisplayField>Sub_Qty_From__c</errorDisplayField>
        <errorMessage>Subscription Quantity From value should be greater than 0</errorMessage>
    </validationRules>
</CustomObject>
