    <fields>
        <fullName>Status_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Status__c, &quot;Inactive&quot;), &quot;Inactive&quot;,
IF(Subscription_Start_Date__c &gt; Subscription_Stop_Date__c, &quot;Deleted&quot;,
IF(Subscription_Stop_Date__c &lt; TODAY(),
IF(ISPICKVAL(Status__c, &quot;Cancelled&quot;) , &quot;Terminated&quot;, &quot;Expired&quot;),
IF(Subscription_Start_Date__c &gt; TODAY(), &quot;Future Active&quot;, &quot;Active&quot;))))</formula>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
