    <validationRules>
        <fullName>Effective_End_Date_should_be_after_the_E</fullName>
        <active>true</active>
        <description>Effective End Date should be after the Effective Start Date</description>
        <errorConditionFormula>Effective_Start_Date__c &gt;= Effective_End_Date__c</errorConditionFormula>
        <errorDisplayField>Effective_End_Date__c</errorDisplayField>
        <errorMessage>Effective End Date is not after the Effective Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_Code_Validation</fullName>
        <active>true</active>
        <description>Promotion Code not allowed to be changed.</description>
        <errorConditionFormula>IF(ISNULL( PRIORVALUE( Promotion_Code__c )),FALSE,ISCHANGED( Promotion_Code__c ) )</errorConditionFormula>
        <errorDisplayField>Promotion_Code__c</errorDisplayField>
        <errorMessage>Promotion Code not allowed to be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_Type_Validation</fullName>
        <active>true</active>
        <description>Do not allow the user to change the Promotion Type.</description>
        <errorConditionFormula>IF(ISNULL( PRIORVALUE( Promotion_Type__c )),FALSE,ISCHANGED( Promotion_Type__c ) )</errorConditionFormula>
        <errorMessage>Promotion Type not allowed to be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_PromoSetup_VR_2_a</fullName>
        <active>true</active>
        <description>a) If Contract is Yes, then contract period must not be blank</description>
        <errorConditionFormula>AND(ISPICKVAL(Contract__c , &apos;Yes&apos;), ISPICKVAL( Contract_Period__c , &apos;&apos;) )</errorConditionFormula>
        <errorDisplayField>Contract_Period__c</errorDisplayField>
        <errorMessage>Contract period must not be blank if contract is Yes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_PromoSetup_VR_2_b</fullName>
        <active>true</active>
        <description>If Contract (Add-On) is Yes, then contract period (Add-On) must not be blank</description>
        <errorConditionFormula>AND(ISPICKVAL( Contract_Add_On__c , &apos;Yes&apos;), ISPICKVAL(  Contract_Period_Add_On__c , &apos;&apos;) )</errorConditionFormula>
        <errorDisplayField>Contract_Period_Add_On__c</errorDisplayField>
        <errorMessage>Contract period (Add-On) must not be blank if Contract (Add-On) is Yes</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Activate</fullName>
        <availability>online</availability>
        <description>Triggers the following validation rules:
1) Add-On Promotion must have atleast 1 package of type: Add-On
2) Gift Promotion must have atleast 1 Gift attached to it.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/apex.js&apos;)}

var packages = sforce.connection.query(&quot;SELECT Name, Promotion__r.Name, Package_Name__r.Name, Package_Name__r.Package_Type__c FROM Promotion_Package__c WHERE Promotion__r.Name != null&quot;);

var promoGifts = sforce.connection.query(&quot;SELECT Name, Promotion__r.Name FROM Promotion_Gift__c WHERE Promotion__c != null&quot;);

var package_count = 0;
var promoGift_count = 0;
var promo = 0;

records = packages.getArray(&quot;records&quot;);
records1 = promoGifts.getArray(&quot;records&quot;);

for (var i=0; i&lt; records.length; i++) 
{
    var record = records[i];
    if(record.Promotion__r.Name == &quot;{!Promotion__c.Name}&quot; &amp;&amp; record.Package_Name__r.Package_Type__c == &quot;Add On&quot;)                      
    {
        package_count = 1;
    }
}

for (var j=0; j&lt; records1.length; j++) 
{
    var record1 = records1[j];
    //alert(records1.Promotion__r.Name);
    if(record1.Promotion__r.Name == &quot;{!Promotion__c.Name}&quot;)                      
    {
        promoGift_count = 1;
    }
}

if(package_count == 0 &amp;&amp; &quot;{!Promotion__c.RecordType}&quot; == &quot;Add-On Promotion&quot;)
{
    alert(&quot;Add-On Promotions must have at least 1 package of type: \&quot;Add-On\&quot;&quot;);
    promo = 1;
}

if(promoGift_count == 0 &amp;&amp; &quot;{!Promotion__c.RecordType}&quot; == &quot;Gift Promotion&quot;)
{
    alert(&quot;Gift Promotions must have at least 1 Gift attached to it.&quot;);
    promo = 1;
}

//Start D-1544 Added by James: Tick/Untick Active checkbox
var newRecords = []; 
var c = new sforce.SObject(&quot;Promotion__c&quot;); 
var isActive =&quot;{!Promotion__c.Active__c}&quot;;
c.id =&quot;{!Promotion__c.Id}&quot;;
if(isActive == 0 &amp;&amp; promo == 0){
	c.Active__c = true;
}else{
	c.Active__c = false;
}
newRecords.push(c); 
result = sforce.connection.update(newRecords);
window.location.reload();
//End D-1544</url>
