<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>List of the column header and its respective Object Name and Field Name of the CSV File used for Bulk Order upload functionality.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Field_Name__c</fullName>
        <description>API name</description>
        <externalId>false</externalId>
        <label>Field Name</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Identifier__c</fullName>
        <description>This will be used to identify its uniqueness among the data being uploaded in order to prevent duplicate record creation. If defined, similar values will be concatenated. Ex. to identify the uniqueness of an Account from other account, we need to specify it by its Name and NRIC.</description>
        <externalId>false</externalId>
        <label>Identifier</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Object_Name__c</fullName>
        <externalId>false</externalId>
        <label>Object Name</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Bulk Order Configuration</label>
    <visibility>Protected</visibility>
</CustomObject>
