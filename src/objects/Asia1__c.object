    <validationRules>
        <fullName>Display_Name_Validation_on_Chars</fullName>
        <active>true</active>
        <description>[KCampang] 08/12/2014. SCR_OrderEntry_CAR_52. Display Name must be between 3 and 50 characters.</description>
        <errorConditionFormula>(LEN( Display_Name__c )&lt;=3 || LEN( Display_Name__c )&gt;= 50) &amp;&amp; ISNEW() &amp;&amp;  ISNULL(Legacy_Created_Date__c)</errorConditionFormula>
        <errorMessage>Display Name must be between 3 and 50 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Display_Name_field_Validation</fullName>
        <active>true</active>
        <description>[KCampang] 08/11/2014. SCR_OrderEntry_CAR_52.</description>
        <errorConditionFormula>NOT(REGEX( Display_Name__c , &quot;[A-Za-z0-9]+([._,-@]?[A-Za-z0-9]+)*$&quot;)) &amp;&amp;  ISNEW() &amp;&amp;  ISNULL(Legacy_Created_Date__c)</errorConditionFormula>
        <errorDisplayField>Display_Name__c</errorDisplayField>
        <errorMessage>Display Name should start with at least one alphanumeric character. It may include a symbol like a dot(.), an underscore(_), a dash (-),  comma(,) or an at sign (@).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LDAP_ID_Validation_on_Number_of_Chars</fullName>
        <active>true</active>
        <description>[KCampang] 08/12/2014. SCR_OrderEntry_CAR_51. Username must be between 8 and 50 characters and cannot be greater than 50 characters.</description>
        <errorConditionFormula>AND(
OR(LEN( User_ID_LDAP_ID__c)&lt;8, LEN( User_ID_LDAP_ID__c)&gt;50 ),
ISNULL(Legacy_Created_Date__c)
)</errorConditionFormula>
        <errorDisplayField>User_ID_LDAP_ID__c</errorDisplayField>
        <errorMessage>Username must be between 8 and 50 characters and cannot be greater than 50 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LDAP_User_ID_Validation</fullName>
        <active>true</active>
        <description>[KCampang] 08/11/2014. SCR_OrderEntry_CAR_51.</description>
        <errorConditionFormula>AND( 
Contact__r.Account.RecordType.Name = &quot;Direct Individual&quot;,
(NOT
	( 
		OR
		(
			(REGEX( User_ID_LDAP_ID__c ,&quot;[A-Za-z0-9]+[._]?[A-Za-z0-9]+$&quot;)), 
			(REGEX(User_ID_LDAP_ID__c , &quot;(.+)@(.+\\.){1,3}[\\w]{2,4}$&quot;))
		)
	)
),
ISNULL(Legacy_Created_Date__c)
)</errorConditionFormula>
        <errorDisplayField>User_ID_LDAP_ID__c</errorDisplayField>
        <errorMessage>LDAP User Id should start with at least one alphanumeric character and end with at least one alphanumeric character. It may include a dot(.) or an underscore(_). It may also be in this form xxxxx@xxxx.xxxx.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Password_Validation_for_Number_of_Chars</fullName>
        <active>true</active>
        <description>[KCampang] 08/12/2014. SCR_OrderEntry_CAR_53. Password is not in the range 5 through 40.</description>
        <errorConditionFormula>AND(
OR(LEN(Password__c)&lt;5, LEN( Password__c)&gt;40),
ISNEW(),
ISNULL(Legacy_Created_Date__c)
)</errorConditionFormula>
        <errorDisplayField>Password__c</errorDisplayField>
        <errorMessage>Password is not in the range 5 through 40.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Password</fullName>
        <active>true</active>
        <description>[KCampang] 08/12/2014. SCR_OrderEntry_CAR_53. Password has to match regex ^[a-zA-Z0-9._]+$</description>
        <errorConditionFormula>AND(
NOT( REGEX( Password__c , &quot;^[a-zA-Z0-9._]+$&quot;)),
ISNEW(),
ISNULL(Legacy_Created_Date__c)
)</errorConditionFormula>
        <errorDisplayField>Password__c</errorDisplayField>
        <errorMessage>Invalid password format.</errorMessage>
    </validationRules>
