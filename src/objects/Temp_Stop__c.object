    <validationRules>
        <fullName>Validate_Restart_Date_1</fullName>
        <active>true</active>
        <description>Restart date must be greater than start date</description>
        <errorConditionFormula>( Restart_Date__c &lt; Case__r.Subscription_Name__r.Zuora__SubscriptionStartDate__c  )</errorConditionFormula>
        <errorMessage>Restart date Should be greater than or equal to sub start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Restart_Date_2</fullName>
        <active>true</active>
        <description>Restart date must be less than or equal to sub stop date</description>
        <errorConditionFormula>( Restart_Date__c &gt; Case__r.Subscription_Name__r.Zuora__SubscriptionEndDate__c  )</errorConditionFormula>
        <errorMessage>Restart date Should be less than or equal to sub stop date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Restart_Date_3</fullName>
        <active>true</active>
        <description>Restart date must  not be greater than subscription end date</description>
        <errorConditionFormula>( Restart_Date__c &gt; Case__r.Subscription_Name__r.Zuora__SubscriptionEndDate__c  )</errorConditionFormula>
        <errorDisplayField>Cancelled__c</errorDisplayField>
        <errorMessage>Restart date Should not be greater than request stop date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Restart_Date_4</fullName>
        <active>false</active>
        <description>Should be greater than or equal to sub start date</description>
        <errorConditionFormula>( Restart_Date__c &lt;  Case__r.Subscription_Name__r.Zuora__SubscriptionStartDate__c  )</errorConditionFormula>
        <errorMessage>Should be greater than or equal to sub start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Restart_Date_5</fullName>
        <active>false</active>
        <description>Restart date should be publication day</description>
        <errorConditionFormula>( Restart_Date__c != POP_Start_Date__c )</errorConditionFormula>
        <errorMessage>Restart date should be publication day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Temp_Stop_Date_1</fullName>
        <active>false</active>
        <description>Temp Stop date must be greater than the current date</description>
        <errorConditionFormula>(Temp_Stop_Date__c &lt;= TODAY() )</errorConditionFormula>
        <errorDisplayField>Cancelled__c</errorDisplayField>
        <errorMessage>Tempstop date should be greater than current date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Temp_Stop_Date_2</fullName>
        <active>true</active>
        <description>Temp Stop date must be less that the subscription end date</description>
        <errorConditionFormula>( Temp_Stop_Date__c &gt;  Case__r.Subscription_Name__r.Zuora__SubscriptionEndDate__c  )</errorConditionFormula>
        <errorDisplayField>Cancelled__c</errorDisplayField>
        <errorMessage>Tempstop date should be less than sub stop date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Temp_Stop_Date_3</fullName>
        <active>true</active>
        <description>Temp Stop date must be within the subscription date</description>
        <errorConditionFormula>( Temp_Stop_Date__c &lt;=  Case__r.Subscription_Name__r.Zuora__SubscriptionStartDate__c )</errorConditionFormula>
        <errorDisplayField>Cancelled__c</errorDisplayField>
        <errorMessage>Tempstop date Should be greater than or equal to sub start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Temp_Stop_Date_4</fullName>
        <active>true</active>
        <description>Temp Stop date must be less than the Restart date</description>
        <errorConditionFormula>Temp_Stop_Date__c    &gt;=   Restart_Date__c</errorConditionFormula>
        <errorDisplayField>Restart_Date__c</errorDisplayField>
        <errorMessage>Restart date should be greater than Temp Stop date</errorMessage>
    </validationRules>
