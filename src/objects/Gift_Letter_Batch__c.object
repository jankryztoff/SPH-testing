    <validationRules>
        <fullName>DTLEndDateLessThanDTLStartDate</fullName>
        <active>true</active>
        <description>D-2503
DTL End Date must not less than DTL Start Date</description>
        <errorConditionFormula>End_Date__c &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>DTL End Date must not less than DTL Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PromoNameNotRequiredForNonChinese</fullName>
        <active>true</active>
        <description>Prevent user from inputting a value in the Promo Chinese Name field if the value of the Letter Template field is not &quot;Premium (Chi)&quot;.</description>
        <errorConditionFormula>NOT(ISPICKVAL(Letter_Template__c, &apos;Premium (Chi)&apos;)) &amp;&amp; Promo_Chinese_Name__c != null</errorConditionFormula>
        <errorDisplayField>Promo_Chinese_Name__c</errorDisplayField>
        <errorMessage>This field should only be populated if the Letter Template is &quot;Premium (Chi)&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PromoNameRequiredForChinese</fullName>
        <active>true</active>
        <description>If the letter template field is set with a value of &quot;Premium (Chi)&quot;, the Promo Chinese Name field must be populated.</description>
        <errorConditionFormula>ISPICKVAL(Letter_Template__c, &apos;Premium (Chi)&apos;) &amp;&amp; Promo_Chinese_Name__c = null &amp;&amp; Promo_Chinese_Name__c = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Promo_Chinese_Name__c</errorDisplayField>
        <errorMessage>Please input the Chinese Name of the selected Promotion in this field. For example, if selected Promotion is Lianhe Zaobao, Promo Chinese Name should be 联合早报.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Redemption_End_Date_Less_Than_Start_Date</fullName>
        <active>true</active>
        <description>D-2503
Redemption End Date must not less than Redemption Start Date</description>
        <errorConditionFormula>Redemption_End_Date__c  &lt;  Redemption_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Redemption_End_Date__c</errorDisplayField>
        <errorMessage>Redemption End Date must not less than Redemption Start Date</errorMessage>
    </validationRules>
